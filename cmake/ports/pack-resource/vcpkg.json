{
  "name": "pack-resource",
  "version": "3",
  "homepage": "https://brisklib.com",
  "license": null,
  "description": "pack_resource compresses binary files and generates C source code for embedding resources",
  "supports": "!(windows & arm32) & !mingw",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "fmt"
    },
    {
      "name": "lz4"
    },
    {
      "name": "zlib"
    },
    {
      "name": "brotli"
    },
    {
        "name": "concurrentqueue"
    },
    {
      "name": "tl-expected"
    },
    {
      "name": "utf8proc"
    },
    {
      "name": "spdlog"
    },
    {
      "name": "rapidjson"
    },
    {
      "name": "msgpack",
      "default-features": false
    },
    {
      "name": "libuv"
    }
  ],
  "overrides": [
    {
      "name": "vcpkg-tool-meson",
      "version-string": "1.3.2",
      "port-version": 8
    },
    {
      "name": "msgpack",
      "version-string": "6.1.1",
      "port-version": 0
    }
  ]
}
