{
    "line_width": 120,
    "tab_size": 4,
    "max_subargs_per_line": 5,
    "separate_ctrl_name_with_space": true,
    "separate_fn_name_with_space": false,
    "dangle_parens": false,
    "max_prefix_chars": 2,
    "max_lines_hwrap": 2,
    "line_ending": "unix",
    "command_case": "canonical",
    "keyword_case": "unchanged",
    "additional_commands": {
        "brisk_metadata": {
            "flags": [
            ],
            "kwargs": {
                "VENDOR": 1,
                "NAME": 1,
                "DESCRIPTION": 1,
                "VERSION": 1,
                "COPYRIGHT": 1,
                "ICON": 1,
                "APPLE_BUNDLE": 1
            }
        },
        "brisk_target_link_resource": {
            "flags": ["BROTLI", "LZ4", "GZIP", "PRIVATE", "PUBLIC"],
            "kwargs": {
                "INPUT": 1
            }
        },
        "brisk_setup_executable": {
            "flags": [],
            "kwargs": {
            },
            "pargs": 1
        }
    },
    "always_wrap": [],
    "algorithm_order": [
        0,
        1,
        2,
        3,
        4
    ],
    "enable_sort": true,
    "autosort": false,
    "hashruler_min_length": 10,
    "per_command": {},
    "bullet_char": "*",
    "enum_char": ".",
    "enable_markup": true,
    "first_comment_is_literal": false,
    "literal_comment_pattern": null,
    "fence_pattern": "^\\s*([`~]{3}[`~]*)(.*)$",
    "ruler_pattern": "^\\s*[^\\w\\s]{3}.*[^\\w\\s]{3}$",
    "canonicalize_hashrulers": true,
    "emit_byteorder_mark": false,
    "input_encoding": "utf-8",
    "output_encoding": "utf-8"
}
